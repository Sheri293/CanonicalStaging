{
  "type": "module",
  "name": "landing-page-seo-auditor",
  "version": "2.0.0",
  "description": "Professional SEO auditing tool for landing pages with comprehensive analysis, reporting, and notifications",
  "main": "run-audit.js",
  "scripts": {
    "audit": "node run-audit.js",
    "audit:staging": "node run-audit.js https://staging-www.electrical.com/ --env=staging",
    "audit:comprehensive": "node run-audit.js https://staging-www.electrical.com/ --include=all --email",
    "audit:comprehensive:email": "node script/email-latest.js",
    "audit:staging-debug": "node run-audit.js https://staging-www.electrical.com/ --include=all --verbose --timeout=60000",
    "audit:full": "node run-audit.js https://staging-www.electrical.com/ --include=all --depth=3 --max-urls=100 --output=excel,html --verbose",
    "audit:visual": "node run-audit.js https://staging-www.electrical.com/ --include=visual",
    "audit:structure": "node run-audit.js https://staging-www.electrical.com/ --include=structure",
    "audit:manipulation": "node scripts/manipulation-detector.js https://staging-www.electrical.com/",
    "baseline:create": "node scripts/baseline-manager.js create https://staging-www.electrical.com/",
    "baseline:create:staging": "node script/baseline-manager.js create https://staging-www.electrical.com/",
    "baseline:clear": "node scripts/baseline-manager.js clear",
    "baseline:clear:visual": "node scripts/baseline-manager.js clear-visual",
    "baseline:clear:structure": "node scripts/baseline-manager.js clear-structure",
    "baseline:list": "node scripts/baseline-manager.js list",
    "detect:manipulation": "node -e \"require('./scripts/manipulation-detector').runManipulationDetection('https://staging-www.electrical.com/')\"",
    "detect:h1-changes": "node -e \"const detector = require('./scripts/manipulation-detector'); detector.runManipulationDetection('https://staging-www.electrical.com/').then(r => { if(r.summary.criticalManipulations > 0) process.exit(1); })\"",
    "setup:baselines": "npm run baseline:create:staging && echo 'Baselines created for staging environment'",
    "deploy:check": "npm run detect:h1-changes || (echo 'SEO manipulation detected - blocking deployment' && exit 1)",
    "test": "echo \"Tests not implemented yet\" && exit 0",
    "setup": "npm install && mkdir -p reports/excel reports/html logs baselines/screenshots baselines/structure current/screenshots diffs/screenshots",
    "start": "npm run audit:comprehensive",
    "dev": "nodemon run-audit.js https://staging-www.electrical.com/",
    "debug": "node --inspect run-audit.js https://staging-www.electrical.com/",
    "help": "node run-audit.js --help",
    "install-browsers": "npx playwright install chromium",
    "postinstall": "npx playwright install chromium && mkdir -p reports/excel reports/html logs baselines/screenshots baselines/structure current/screenshots diffs/screenshots",
    "clean": "rm -rf reports/* logs/*",
    "clean:baselines": "rm -rf baselines/* current/* diffs/*",
    "clean:all": "npm run clean && npm run clean:baselines",
    "lint": "eslint src/ --ext .js",
    "format": "prettier --write src/**/*.js",
    "validate": "npm run lint && echo 'Validation passed'"
  },
  "config": {
    "test_url": "https://staging-www.electrical.com/",
    "staging_url": "https://staging-www.electrical.com/",
    "production_url": "https://www.electrical.com/"
  },
  "bin": {
    "seo-audit": "./run-audit.js"
  },
  "keywords": [
    "seo",
    "audit",
    "landing-page",
    "crawler",
    "canonical",
    "meta-tags",
    "structured-data",
    "broken-links",
    "redirects",
    "performance",
    "accessibility",
    "playwright",
    "automation",
    "testing",
    "sdet",
    "quality-assurance",
    "web-audit",
    "seo-analyzer",
    "lighthouse",
    "core-web-vitals",
    "visual-regression",
    "html-structure",
    "seo-manipulation-detection"
  ],
  "author": {
    "name": "SEO Audit Tool Developer"
  },
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "colors": "^1.4.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "fs-extra": "^11.2.0",
    "handlebars": "^4.7.8",
    "jsdom": "^23.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "nodemailer": "^6.9.7",
    "p-limit": "^4.0.0",
    "p-queue": "^7.4.1",
    "pixelmatch": "^5.3.0",
    "playwright": "^1.40.0",
    "pngjs": "^7.0.0",
    "uuid": "^9.0.1",
    "validator": "^13.11.0",
    "winston": "^3.11.0",
    "xml2js": "^0.6.2",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "eslint": "^8.55.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0"
  },
  "optionalDependencies": {
    "lighthouse": "^11.4.0",
    "puppeteer": "^21.6.1"
  },
  "files": [
    "run-audit.js",
    "src/",
    "config/",
    "scripts/",
    "README.md",
    ".env.example"
  ]
}
